{
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "alwaysStrict": true,
    "declaration": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "target": "ES6",
    "sourceMap": true,
    "allowJs": false,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "msgpack": [
        "node_modules/msgpack-js/msgpack"
      ]
    }
  },
  "exclude": [
    "dist",
    "tests"
  ]
}
